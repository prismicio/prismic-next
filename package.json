{
	"name": "@prismicio/next",
	"version": "2.1.1",
	"description": "Helpers to integrate Prismic into Next.js apps",
	"keywords": [
		"typescript",
		"prismic",
		"next",
		"vercel",
		"preview"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/prismicio/prismic-next.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": "./dist/index.cjs",
		"./pages": "./dist/pages.cjs",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.cts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/index.d.ts"
			],
			"pages": [
				"./dist/pages/index.d.ts"
			],
			"react-server": [
				"./dist/react-server/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"workspaces": [
		".",
		"e2e-projects/*"
	],
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch",
		"e2e": "playwright test",
		"e2e:ui": "npm run e2e -- --ui",
		"format": "prettier --write .",
		"lint": "oxlint --deny-warnings",
		"prepare": "npm run build",
		"test": "npm run lint && npm run types && npm run build && npm run e2e",
		"types": "tsc --noEmit"
	},
	"dependencies": {
		"@prismicio/simulator": "^0.1.4",
		"imgix-url-builder": "^0.0.5",
		"lz-string": "^1.5.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.49.1",
		"@prismicio/client": "^7.12.0",
		"@trivago/prettier-plugin-sort-imports": "^6.0.2",
		"@types/node": "^22.10.2",
		"@types/react": "^19.2.3",
		"next": "16.0.8",
		"oxlint": "^1.38.0",
		"playwright": "^1.49.1",
		"prettier": "^3.4.2",
		"prettier-plugin-jsdoc": "^1.3.0",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"tsdown": "^0.19.0-beta.5",
		"typescript": "^5.7.2"
	},
	"peerDependencies": {
		"@prismicio/client": "^7",
		"next": "^13.4.5 || ^14 || ^15 || ^16",
		"react": "^18 || ^19"
	},
	"engines": {
		"node": ">=20"
	},
	"publishConfig": {
		"access": "public"
	}
}
